                                        <div class="text"><h3>Photography </h3></div>
                                    </div>
                                </div>
                            </div>
                        </a>
                        <a href=@Url.Action("design","Portfolio")>
                            <div class="col-md-6" style="padding-right:0; padding-left:0; margin:0">

                                <div class="row over" style="margin:0;">
                                    <img src="~/Images/slide2.jpg" class="image img-responsive">
                                    <div class="overlay">
                                        <div class="text"><h3>Graphic Design </h3></div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div><!--row-->
                    <div class="row">
                        <a href=@Url.Action("Code","Portfolio")>
                            <div class="col-md-6" style="padding-right:0; padding-left:0; margin:0">
                                <div class="row over" style="margin:0;">
