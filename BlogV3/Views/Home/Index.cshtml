                    </div>
                </div>
                <br>
                <div>
                    <span class="glyphicon glyphicon-chevron-down" aria-hidden="true" style="text-align:center;"></span>
                </div>
                <br>

                <div class="tag-holder" style="margin-bottom:50px;">
                    <div id="tag" class="container-fluid">
                        <h2>Say Hi!</h2>
                    </div>
                    <a name="contact"></a>
                    <div class="row " style="border: solid 1px #e2e4e8; margin-bottom:50px;">

                        <br>
                        <div class="col-md-offset-2 col-md-8">
                            <form action="mailto:AliciaMacCara%40gmail%2ecom" method="post" enctype="text/plain">
                                <div class="form-group">
                                    <p>Name:</p>
                                    <input type="text" class="form-control" name="Name " placeholder="Jane Doe">
                                </div>
                                <div class="form-group">
                                    <p>Email Address:</p>
                                    <input type="text" class="form-control" name="Email " placeholder="JaneDoe@email.com">
                                </div>
                                <div class="form-group">
                                    <p>Message:</p>
                                    <textarea class="form-control" name="Message " placeholder="Hi there!" cols="100" rows="10"></textarea>
                                </div>

                                <button class="btn btn-default" type="submit" style="margin-bottom:20px; color:#6ba2d7">Send</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            @Html.Partial("SideBar")
        </div>

    </div>
</div>
